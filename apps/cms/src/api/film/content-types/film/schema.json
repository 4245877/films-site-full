{
  "kind": "collectionType",
  "collectionName": "films",
  "info": {
    "singularName": "film",
    "pluralName": "films",
    "displayName": "film"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "year": {
      "type": "integer"
    },
    "description": {
      "type": "text"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "film_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::film-type.film-type",
      "inversedBy": "films"
    }
  }
}
